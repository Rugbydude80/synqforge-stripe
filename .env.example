# SynqForge — Environment Example
#
# Copy this file to `.env.local` for local development.
#
# Safety & Demo Mode
# - Leaving Supabase public URL or anon key empty enables Demo Mode (no external services; in‑memory stores + fixtures).
# - Do not commit real secrets. This file is safe to commit.

# App URLs
NEXT_PUBLIC_SITE_URL="http://localhost:3000"
# Optional fallback used for telemetry headers
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Feature flags
# Enables Freelancer Mode surfaces alongside existing organisation/project features
NEXT_PUBLIC_FREELANCER_MODE="false"
# Disable billing by default in dev; set to "false" to enable
DISABLE_BILLING="true"
NEXT_PUBLIC_DISABLE_BILLING="true"

# Supabase (leave blank to run in Demo Mode)
NEXT_PUBLIC_SUPABASE_URL=""
NEXT_PUBLIC_SUPABASE_ANON_KEY=""
# Server‑side only; not required for Demo Mode
SUPABASE_SERVICE_ROLE_KEY=""

# Stripe (optional for Demo Mode)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=""
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY_LIVE=""
STRIPE_SECRET_KEY=""
STRIPE_SECRET_KEY_LIVE=""
STRIPE_WEBHOOK_SECRET=""

# OpenRouter (AI)
# Default model: google/gemini-1.5-flash
# Strict schema: openai/gpt-4o-mini
# Fallback: deepseek/deepseek-chat
OPENROUTER_API_KEY=""

# Upstash Redis (optional; used for rate limiting and AI caching)
UPSTASH_REDIS_REST_URL=""
UPSTASH_REDIS_REST_TOKEN=""

# Realtime (optional)
# Format: keyName:keySecret
ABLY_API_KEY=""

# Inngest (optional)
INNGEST_APP_ID="synqforge-stripe"

# Experimental model fallback (optional)
USE_LLAMACHAIN=""

NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

STRIPE_SECRET_KEY=

INNGEST_APP_ID=synqforge-stripe
SUPABASE_AUTH_EXTERNAL_GITHUB_REDIRECT_URI="http://127.0.0.1:54321/auth/v1/callback"
SUPABASE_AUTH_EXTERNAL_GITHUB_CLIENT_ID=
SUPABASE_AUTH_EXTERNAL_GITHUB_SECRET=

# AI and caching configuration
# API key for OpenRouter (used for AI story generation)
OPENROUTER_API_KEY=

# Upstash Redis configuration for caching and rate limiting
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=
